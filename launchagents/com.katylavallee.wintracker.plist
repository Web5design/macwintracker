<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>label</key>
    <string>com.katylavallee.wintracker</string>
    <key>ProgramArguments</key>
    <array>
        <string>/Users/{{USER}}/Library/Application Support/com.katylavallee.wintracker/wintracker.py</string>
        <string>{{LIMIT}}</string>
        <string>{{INTERVAL}}</string>
    </array>
    <key>Environment Variables</key>
    <dict>
      <key>PATH</key>
      <string>{{PATH}}</string>
      <key>PYTHONPATH</key>
      <string>{{PYTHONPATH}}</string>
    </dict>
    <key>StandardErrorPath</key>
    <string>/Users/{{USER}}/Library/Logs/com.katylavallee.wintracker/wintracker_err.log</string>
    <key>StandardOutPath</key>
    <string>/Users/{{USER}}/Library/Logs/com.katylavallee.wintracker/wintracker.log</string>
    <key>StartInterval</key>
    <integer>{{LAUNCHDINTVL}}</integer>
  </dict>
</plist>
